        </div>
      </div>
      <div class="avatar-container column">
        <img class="red-avatar" src="/public/coloredAvatarCircleCutout.png"/>
        <img id="blue" class="blue-avatar" src="/public/blue.png"/>
      </div>
      <div class="nav-container column">
        <div class="nav-content">
          <ul>
            <li><a id="workBtn"><div id="work" class="item-one">My Work</div></a></li>
            <li><a id="linkedin"><div id="linkedin" class="item-three">Linkedin</div></a></li>
            <li><a id="github"><div id="github" class="item-four">Github</div></a></li>

          </ul>
        </div>
      </div>
    </div>
  </body>
</html>
<script src="/javascript/functions.js"></script>